# Sample .env file for local development
# Copy this file to .env and update with your values

# ================================
# REQUIRED CONFIGURATION
# ================================

# Your Google Cloud Project ID (REQUIRED)
GOOGLE_CLOUD_PROJECT=my-food-delivery-project

# ================================
# OPTIONAL CONFIGURATION
# ================================

# Google Cloud Region
GOOGLE_CLOUD_REGION=eur3

# Service name
SERVICE_NAME=food-delivery-mcp

# For local Firestore emulator development
# FIRESTORE_EMULATOR_HOST=localhost:8081

# Cloud Run deployment settings
MEMORY=1Gi
CPU=1
MAX_INSTANCES=100
TIMEOUT=300

MCP_SERVER_NAME="FoodieApp Assistant"
MCP_SERVER_HOST="0.0.0.0"
MCP_SERVER_PORT=8050